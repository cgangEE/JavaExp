import javax.swing.*;

import java.awt.*;
import java.io.*;
import java.util.*;

public class Client {
	public static void main(String args[]){
		JFrame mw = new JFrame("Client");
		JTextArea textA=new JTextArea("",5,10);
		
		
		
		JButton button = new JButton("ok");
		mw.add(button); mw.add(textA);
		
		mw.setSize(500,500);
		mw.pack();
		
		mw.setBackground(Color.black);
		mw.setVisible(true);
	}
}
